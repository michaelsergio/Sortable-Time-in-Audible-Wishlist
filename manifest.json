{
     "name": "Sortable Time in Audible Wishlist",
     "version": "1.2.0",
     "manifest_version": 2,
     "description": "Adds a time column to each page of the Audible Wishlist.",
     "author": "Michael Sergio <mikeserg@gmail.com>",

     "icons": {  "16": "icon16.png",
                 "48": "icon48.png",
                "128": "icon128.png" },
     "content_scripts": [{
         "js": ["audible.user.js"],
         "matches": [
           "*://www.audible.com/wl*",
           "*://www.audible.co.uk/wl*",
           "*://www.audible.de/wl*",
           "*://www.audible.fr/wl*",
           "*://www.audible.com.au/wl*",
           "*://www.audible.it/wl*",
           "*://www.audible.co.jp/wl*"
         ]
      }],
     "permissions": [
         "storage",
         "*://www.audible.com/*",
         "*://www.audible.co.uk/wl*",
         "*://www.audible.de/wl*",
         "*://www.audible.fr/wl*",
         "*://www.audible.com.au/wl*",
         "*://www.audible.it/wl*",
         "*://www.audible.co.jp/wl*"
      ]
}
